{
  "name": "qiankun-link",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start-all": "npm-run-all --parallel start:*",
    "install-all": "npm-run-all --serial install:*",
    "install:main": "cd main && yarn",
    "install:subapp": "cd subapp && yarn",
    "start:main": "cd main && rimraf dist && yarn run start",
    "start:main-build": "cd main && rimraf prod-dist && yarn run build && serve -l 5679 -s prod-dist",
    "start:subapp": "cd subapp && yarn run serve"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "serve": "^11.3.0"
  }
}
